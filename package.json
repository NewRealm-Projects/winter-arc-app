{
  "name": "winter-arc-tracker",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:code && npm run lint:secrets",
    "lint:code": "eslint .",
    "lint:secrets": "node scripts/check-secrets.mjs",
    "format": "eslint . --fix",
    "lint:unused": "eslint 'src/**/*.{ts,tsx}'",
    "scan:knip": "knip",
    "scan:tsprune": "ts-prune",
    "scan:dep": "depcheck",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:all": "npm run typecheck && npm run lint && npm run test:unit && npm run test:ui",
    "e2e": "playwright test --config e2e/playwright.config.ts",
    "e2e:report": "playwright show-report e2e/artifacts/playwright-report",
    "e2e:ui": "playwright test --config e2e/playwright.config.ts --ui",
    "analyze": "vite build && npx rollup-plugin-visualizer stats.html --open",
    "perf:budget": "node scripts/check-budgets.mjs",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci:upload": "lhci upload",
    "lhci:run": "npm run build && npm run preview -- --port 4173 & sleep 3 && lhci autorun && kill $!",
    "agent:lighthouse": "node .agent/scripts/run-lighthouse.mjs",
    "agent:bundle": "node .agent/scripts/bundle-report.mjs",
    "agent:artifacts": "node .agent/scripts/prepare-artifacts.mjs",
    "clean": "node scripts/cleanup-repo.mjs",
    "clean:dry-run": "node scripts/cleanup-repo.mjs --dry-run",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@sentry/react": "10.18.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "firebase": "^12.3.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.37.0",
    "@lhci/cli": "0.15.1",
    "@playwright/test": "^1.56.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.10.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "knip": "^5.64.2",
    "msw": "^2.11.4",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^6.0.4",
    "tailwindcss": "^3.4.17",
    "ts-prune": "^0.10.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "wait-on": "^9.0.1",
    "workbox-window": "^7.3.0"
  }
}
