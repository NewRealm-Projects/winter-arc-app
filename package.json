{
  "name": "winter-arc-tracker",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:unused": "eslint 'src/**/*.{ts,tsx}'",
    "scan:knip": "knip",
    "scan:tsprune": "ts-prune",
    "scan:dep": "depcheck",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:ui": "playwright test",
    "test:all": "npm run typecheck && npm run lint && npm run test:unit && npm run test:ui",
    "analyze": "vite build && npx rollup-plugin-visualizer stats.html --open",
    "perf:budget": "node scripts/check-budgets.mjs",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci:upload": "lhci upload",
    "lhci:run": "npm run build && npm run preview -- --port 4173 & sleep 3 && lhci autorun && kill $!",
    "agent:lighthouse": "node .agent/scripts/run-lighthouse.mjs",
    "agent:bundle": "node .agent/scripts/bundle-report.mjs",
    "agent:artifacts": "node .agent/scripts/prepare-artifacts.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@sentry/react": "^10.17.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.15.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.17.0",
    "@lhci/cli": "0.15.1",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.10.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "knip": "^5.64.1",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^6.0.4",
    "tailwindcss": "^3.4.17",
    "ts-prune": "^0.10.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "workbox-window": "^7.3.0"
  }
}
